{
    "name": "randionfernandez/comunidades",
    "type": "project",
    "description": "Gesti√≥n de comunidades de vecinos.",
    "authors": [
        {
            "name": "randion",
            "email": "randion@cifpfbmoll.eu",
	    "role" : "Developer"
        }
    ],
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "dragon-code/pretty-array": "^3.0",
        "dragon-code/support": "^5.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel-lang/lang": "^9.0",
        "laravel-lang/publisher": "^12.2",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.0",
        "symfony/mailer": "^6.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "laravel/dusk": "^6.18",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories"
        ],
        "files": ["app/Http/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
